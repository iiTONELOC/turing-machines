{
  "name": "turing-machines",
  "version": "1.0.0",
  "main": "dist/turing-machines.cjs.js",
  "module": "dist/turing-machines.esm.js",
  "browser": "dist/turing-machines.umd.js",
  "type": "module",
  "types": "./src/turingMachines/singleTape/singleTM.d.ts",
  "scripts": {
    "test-dev": "node --test --watch",
